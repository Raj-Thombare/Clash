{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node ./dist/index.js",
    "server": "nodemon ./dist/index.js",
    "watch": "tsc -w",
    "dev": "concurrently \"npm run watch\" \"npm run server\" \"npm run watch-views\"",
    "build": "tsc && npm run copy-files",
    "copy-files": "copyfiles -u 1 \"src/views/**/*\" dist/",
    "watch-views": "nodemon --watch src/views -e ejs -x \" npm run copy-files\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^6.2.0",
    "bullmq": "^5.34.9",
    "concurrently": "^9.1.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "prisma": "^6.2.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/node": "^22.10.5",
    "copyfiles": "^2.4.1",
    "nodemon": "^3.1.9",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  }
}
